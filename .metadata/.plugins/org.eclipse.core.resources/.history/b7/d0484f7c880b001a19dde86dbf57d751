public class MyApp {

	public static void main(String[] args) {
		
		ClassPathXmlApplicationContext context = new ClassPathXmlApplicationContext("applicationContext.xml")

		Coach coach = new TrackCouch();

		System.out.println(coach.getDailyWorkout());

	}

}
